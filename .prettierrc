{
  "tabWidth": 2,
  "useTabs": false,
  "overrides": [
    {
      "files": ["*.json"],
      "options": {
        "parser": "json",
        "tabWidth": 2,
        "useTabs": false,
        "quoteProps": "preserve",
        "trailingComma": "none"
      }
    },
    {
      "files": ["*.json5"],
      "options": {
        "parser": "json5",
        "tabWidth": 2,
        "useTabs": false,
        "quoteProps": "preserve"
      }
    }
  ]
}
